    	 7.13.o \
	 7.27.o \
	 7.31.o \
	 7.41.o
	g++ 7.7.o -o 7.7
	g++ 7.13.o -o 7.13
	g++ 7.27.o -o 7.27
	g++ 7.31.o -o 7.31
	g++ 7.41.o -o 7.41

