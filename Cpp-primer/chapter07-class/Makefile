	g++ 7.7.cpp -c -std=c++11

7.13.o: 7.13.cpp 7.12.h
	g++ 7.13.cpp -c -std=c++11

7.27.o: 7.27.cpp 7.24.h
	g++ 7.27.cpp -c -std=c++11

7.31.o: 7.31.cpp
	g++ 7.31.cpp -c -std=c++11

7.41.o: 7.41.cpp
	g++ 7.41.cpp -c -std=c++11

.PHONY: clean

clean:
	rm 7.7 7.7.o \
	   7.13 7.13.o \
